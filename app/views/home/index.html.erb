<div class="row ">
    <% if user_signed_in?%>
        <%= render 'tweets/form', tweet: @tweet %>
    <%end%>
   
    <% @tweets.each do |tweet| %>
            <%= render 'tweets/single_tweet', tweet: tweet %>
    <% end %> 
 
</div>

<div class=" my-4">
    <div class="d-flex justify-content-center">
        <%= paginate @tweets %>
    </div>
</div>
